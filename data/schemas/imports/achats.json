{
  "$schema": "https://frictionlessdata.io/schemas/table-schema.json",
  "encoding": "utf-8",
  "fields": [
    {
      "constraints": {
        "pattern": "^[0-9]{14}$",
        "required": true
      },
      "description": "La cantine avec ce SIRET doit être déjà enregistrée sur notre plateforme.",
      "example": "49463556926130",
      "format": "default",
      "name": "siret",
      "title": "SIRET de la cantine ayant réalisé l'achat",
      "type": "string"
    },
    {
      "constraints": {
        "required": true
      },
      "description": "",
      "example": "Pommes de terre",
      "format": "default",
      "name": "description",
      "title": "Description de l'achat",
      "type": "string"
    },
    {
      "constraints": {
        "required": true
      },
      "description": "",
      "example": "Le traiteur du village",
      "format": "default",
      "name": "fournisseur",
      "title": "Nom du fournisseur",
      "type": "string"
    },
    {
      "constraints": {
        "required": true
      },
      "description": "",
      "example": "2022-01-30",
      "format": "%Y-%m-%d",
      "name": "date",
      "title": "Date de l'achat",
      "type": "date"
    },
    {
      "constraints": {
        "required": true
      },
      "description": "",
      "example": "1234.99",
      "format": "default",
      "name": "prix_ht",
      "title": "Prix HT de l'achat",
      "type": "number"
    },
    {
      "constraints": {
        "enum": [
          "VIANDES_VOLAILLES",
          "CHARCUTERIE",
          "PRODUITS_DE_LA_MER",
          "FRUITS_ET_LEGUMES",
          "PRODUITS_LAITIERS",
          "BOULANGERIE",
          "BOISSONS",
          "AUTRES"
        ],
        "required": false
      },
      "description": "",
      "example": "VIANDES_VOLAILLES",
      "format": "default",
      "name": "famille_produits",
      "title": "Famille de produits de l'achat",
      "type": "string"
    },
    {
      "constraints": {
        "enum": [
          "BIO",
          "LABEL_ROUGE",
          "AOCAOP",
          "IGP",
          "STG",
          "HVE",
          "PECHE_DURABLE",
          "RUP",
          "COMMERCE_EQUITABLE",
          "FERMIER",
          "EXTERNALITES",
          "PERFORMANCE",
          "FRANCE",
          "SHORT_DISTRIBUTION",
          "LOCAL"
        ],
        "enum_multiple": true,
        "enum_multiple_seperator": ",",
        "pattern": "(?:(?:^|;)(BIO|LABEL_ROUGE|AOCAOP|IGP|STG|HVE|PECHE_DURABLE|RUP|COMMERCE_EQUITABLE|FERMIER|EXTERNALITES|PERFORMANCE|FRANCE|SHORT_DISTRIBUTION|LOCAL))+$",
        "required": false
      },
      "description": "",
      "example": "BIO,IGP",
      "format": "default",
      "name": "caracteristiques",
      "title": "Caractéristiques de l'achat",
      "type": "string"
    },
    {
      "constraints": {
        "enum": [
          "AUTOUR_SERVICE",
          "DEPARTMENT",
          "REGION",
          "AUTRE"
        ],
        "required": false
      },
      "description": "Obligatoire si l'achat a la caractéristique de LOCAL.",
      "example": "AUTOUR_SERVICE",
      "format": "default",
      "name": "definition_local",
      "title": "Définition de local",
      "type": "string"
    }
  ],
  "format": "csv",
  "homepage": "https://github.com/betagouv/ma-cantine/refs/heads/main/data/schemas/imports/cantines.json",
